<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.MyFlight">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyFlight">
        <activity
            android:name="com.example.MyFlight.SettingActivity"
            android:label="@string/title_activity_setting"
            android:theme="@style/Theme.MyFlight.NoActionBar"></activity>
        <activity android:name="com.example.MyFlight.BookActivity" />
        <activity android:name="com.example.MyFlight.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.MyFlight.RouteInfoActivity" />
        <activity android:name="com.example.MyFlight.FligthInfoActivity" />
        <activity android:name="com.example.MyFlight.SignUpActivity" />
        <activity android:name="com.example.MyFlight.MainActivity" />
    </application>

</manifest>